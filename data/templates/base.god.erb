<% engine.procfile.entries.each do |process| %>
  <% 1.upto(concurrency[process.name]) do |num| %>
God.watch do |w|
  w.name = "<%= "#{process.name}-#{num}" %>"
  w.start = "<%= process.command %>"
  w.keepalive
  w.group = "<%= process.name %>"
    <%= extension(process.name) %>
end
  <% end %>
<% end %>
